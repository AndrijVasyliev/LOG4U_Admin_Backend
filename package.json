        "@nestjs/passport": "^10.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/schedule": "^2.2.0",
        "@nestjs/serve-static": "^4.0.0",
        "@nestjs/terminus": "^9.1.1",
        "@willsoto/nestjs-prometheus": "^4.7.0",
        "class-transformer": "^0.5.1",
