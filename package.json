        "reflect-metadata": "^0.1.13",
        "response-time": "^2.3.2",
        "rxjs": "^7.8.0",
        "tslib": "^2.4.0",
        "winston": "^3.8.1"
    },
    "devDependencies": {
        "@golevelup/ts-jest": "^0.3.5",
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
