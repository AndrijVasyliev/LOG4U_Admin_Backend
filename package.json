    "jest": {
        "globalSetup": "./bootstrap.tests.ts",
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "testTimeout": 50000,
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s",
            "!<rootDir>/coverage/"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
